<?xml version="1.0"?>
<Patch>
  <!-- Wheat -->
  <Operation Class="PatchOperationAdd" MayRequire="vanillaexpanded.vcooke">
    <xpath>Defs/ThingDef[defName="VCE_Wheat"]/plant</xpath>
    <value>
      <immatureGraphicPath>Things/Plants/Crops/WheatPlant_Immature</immatureGraphicPath>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace" MayRequire="vanillaexpanded.vcooke">
    <xpath>Defs/ThingDef[defName="VCE_Wheat"]/plant/harvestedThingDef</xpath>
    <value>
      <harvestedThingDef>MOL_RawWheat</harvestedThingDef>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace" MayRequire="vanillaexpanded.vcooke">
    <xpath>Defs/ThingDef[defName="VCE_Wheat"]/graphicData/texPath</xpath>
    <value>
      <texPath>Things/Plants/Crops/WheatPlant</texPath>
    </value>
  </Operation>
  <!-- Sugarcane -->
  <Operation Class="PatchOperationReplace" MayRequire="vanillaexpanded.vcooke">
    <xpath>Defs/ThingDef[defName="VCE_Sugarcane"]/graphicData/texPath</xpath>
    <value>
      <texPath>Things/Plants/Crops/SugarCanePlant</texPath>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace" MayRequire="vanillaexpanded.vcooke">
    <xpath>Defs/ThingDef[defName="VCE_Sugarcane"]/plant/harvestedThingDef</xpath>
    <value>
      <harvestedThingDef>MOL_RawSugarcane</harvestedThingDef>
    </value>
  </Operation>
  <!-- Cheese -->
  <Operation Class="PatchOperationReplace" MayRequire="vanillaexpanded.vcooke">
    <xpath>Defs/ThingDef[defName="VCE_Cheese"]/description</xpath>
    <value>
      <description>Cheese is a dairy product, derived from milk and produced in a wide range of flavors, textures and forms by coagulation of the milk protein casein. Cheese is valued for its portability, long life, and high content of fat, protein, calcium, and phosphorus. It is more compact and has a longer shelf life than milk.</description>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace" MayRequire="vanillaexpanded.vcooke">
    <xpath>Defs/ThingDef[defName="VCE_Cheese"]/graphicData/texPath</xpath>
    <value>
      <texPath>Things/Item/Resource/ArtisanalGoods/Cheese</texPath>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace" MayRequire="vanillaexpanded.vcooke">
    <xpath>Defs/ThingDef[defName="VCE_Cheese"]/statBases</xpath>
    <value>
      <statBases>
        <MarketValue>7</MarketValue>
        <Nutrition>0.1</Nutrition>
      </statBases>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace" MayRequire="vanillaexpanded.vcooke">
    <xpath>Defs/ThingDef[defName="VCE_Cheese"]/comps</xpath>
    <value>
      <comps>
        <li Class="CompProperties_Rottable">
          <daysToRotStart>120</daysToRotStart>
          <rotDestroys>true</rotDestroys>
        </li>
        <li Class="CompProperties_Ingredients">
			
			</li>
        <li Class="VEF.Things.CompProperties_LabelByIngredients">
          <overrides>
            <li>
              <key>InsectJelly</key>
              <value>Insect</value>
            </li>
          </overrides>
          <exclusions>
            <li>Milk</li>
          </exclusions>
        </li>
      </comps>
    </value>
  </Operation>
  <!-- Mayo -->
  <Operation Class="PatchOperationReplace" MayRequire="vanillaexpanded.vcooke">
    <xpath>Defs/ThingDef[defName="VCE_Mayo"]/graphicData/texPath</xpath>
    <value>
      <texPath>Things/Item/Resource/ArtisanalGoods/Mayonnaise</texPath>
    </value>
  </Operation>
  <!-- Soup pot -->
  <Operation Class="PatchOperationReplace" MayRequire="vanillaexpanded.vcooke">
    <xpath>Defs/ThingDef[defName="VCE_ElectricPot"]/comps</xpath>
    <value>
      <comps>
        <li Class="CompProperties_Power">
          <compClass>CompPowerTrader</compClass>
          <shortCircuitInRain>false</shortCircuitInRain>
          <basePowerConsumption>100</basePowerConsumption>
        </li>
        <li Class="CompProperties_Flickable"/>
        <li Class="ProductionExpanded.CompProperties_ResourceProcessor">
          <maxCapacity>1</maxCapacity>
          <hasIdlePowerCost>false</hasIdlePowerCost>
          <usesOnTexture>true</usesOnTexture>
          <keepOnTextureOnFinish>true</keepOnTextureOnFinish>
          <showOnTextureWhenPaused>true</showOnTextureWhenPaused>
          <hasTempRequirements>false</hasTempRequirements>
          <soundInput>Corpse_Drop</soundInput>
          <soundStartCycle>FlickSwitch</soundStartCycle>
          <showGizmo>false</showGizmo>
          <soundExtract>Standard_Drop</soundExtract>
          <onTextureKeyWord>Full</onTextureKeyWord>
        </li>
      </comps>
    </value>
  </Operation>
</Patch>
