<?xml version="1.0"?>
<Patch>
  <!-- Remove VCE_CheeseGraphicOffsets to prevent duplicate "Milk" key conflict with MOL_CheeseGraphicOffsets -->
  <!-- This MUST be done via XML patch (not C#) because it needs to run before VEF.Graphics.StaticCollectionsClass initializes -->
  <Operation Class="PatchOperationRemove" MayRequire="vanillaexpanded.vcooke">
    <xpath>Defs/VEF.Graphics.GraphicOffsets[defName="VCE_CheeseGraphicOffsets"]</xpath>
  </Operation>
</Patch>
