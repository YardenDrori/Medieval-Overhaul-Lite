<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- ==================== Medicinal Herbs ======================= -->
  <ThingDef ParentName="MedicineBase">
    <defName>MOL_WinterBalm</defName>
    <label>winterbalm</label>
    <description>A pack of herbal concoctions extracted from the winterbalm plant. It is less potent than industrial pharmaceuticals, but much better than nothing.</description>
    <possessionCount>4</possessionCount>
    <graphicData>
      <texPath>Things/Item/Resource/MOL_WinterBalm</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
      <drawSize>1</drawSize>
    </graphicData>
    <techLevel>Neolithic</techLevel>
    <statBases>
      <MarketValue>10</MarketValue>
      <Mass>0.35</Mass>
      <Flammability>1.3</Flammability>
      <MedicalPotency>0.60</MedicalPotency>
      <MedicalQualityMax>0.70</MedicalQualityMax>
      <DeteriorationRate>6</DeteriorationRate>
    </statBases>
    <tickerType>Rare</tickerType>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>150</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
    <allowedArchonexusCount>20</allowedArchonexusCount>
  </ThingDef>
