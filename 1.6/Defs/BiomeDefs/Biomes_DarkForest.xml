<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <BiomeDef>
    <defName>MOL_DarkForest</defName>
    <label>Dark Forest</label>
    <description>An overgrown exclusion zone from a long-abandoned bioweapons research facility. Towering trees with accelerated growth genes block most sunlight, creating a dim, claustrophobic environment beneath the canopy. Thorn-laden undergrowth and genetically-enhanced predators make exploration a lethal proposition. Few colonists venture deep into the forest. Fewer return.</description>
    <workerClass>MOExpandedLite.BiomeWorker_DarkForest</workerClass>
    <animalDensity>2.8</animalDensity>
    <plantDensity>0.85</plantDensity>
    <implemented>true</implemented>
    <forageability>1</forageability>
    <foragedFood>RawBerries</foragedFood>
    <settlementSelectionWeight>1</settlementSelectionWeight>
    <lakeBeachTerrain>Mud</lakeBeachTerrain>
    <mudTerrain>Mud</mudTerrain>
    <riverbankTerrain>Riverbank</riverbankTerrain>
    <riverbankSizeRange>6~8</riverbankSizeRange>
    <texture>Biomes/DarkForest</texture>
    <maxFishPopulation>500</maxFishPopulation>
    <fishTypes MayRequire="Ludeon.RimWorld.Odyssey">
      <freshwater_Common>
        <Fish_Salmon MayRequire="Ludeon.RimWorld.Odyssey">1</Fish_Salmon>
      </freshwater_Common>
      <freshwater_Uncommon>
        <Fish_Frostfish MayRequire="Ludeon.RimWorld.Odyssey">1</Fish_Frostfish>
      </freshwater_Uncommon>
      <saltwater_Common>
        <Fish_Salmon MayRequire="Ludeon.RimWorld.Odyssey">1</Fish_Salmon>
        <Fish_Cod MayRequire="Ludeon.RimWorld.Odyssey">1</Fish_Cod>
      </saltwater_Common>
      <saltwater_Uncommon>
        <Fish_Dogfish MayRequire="Ludeon.RimWorld.Odyssey">1</Fish_Dogfish>
      </saltwater_Uncommon>
      <rareCatchesSetMaker MayRequire="Ludeon.RimWorld.Odyssey">RareFishingCatches_Cold</rareCatchesSetMaker>
    </fishTypes>
    <soundsAmbient>
      <li>AmbientNightInsects_Standard</li>
    </soundsAmbient>
    <diseaseMtbDays>45</diseaseMtbDays>
    <diseases>
      <li>
        <diseaseInc>Disease_Flu</diseaseInc>
        <commonality>100</commonality>
      </li>
      <li>
        <diseaseInc>Disease_Plague</diseaseInc>
        <commonality>100</commonality>
      </li>
      <li>
        <diseaseInc>Disease_Malaria</diseaseInc>
        <commonality>100</commonality>
      </li>
      <li>
        <diseaseInc>Disease_FibrousMechanites</diseaseInc>
        <commonality>0</commonality>
      </li>
      <li>
        <diseaseInc>Disease_SensoryMechanites</diseaseInc>
        <commonality>0</commonality>
      </li>
      <li>
        <diseaseInc>Disease_GutWorms</diseaseInc>
        <commonality>50</commonality>
      </li>
      <li>
        <diseaseInc>Disease_MuscleParasites</diseaseInc>
        <commonality>50</commonality>
      </li>
    </diseases>
    <terrainsByFertility>
      <li>
        <terrain>Soil</terrain>
        <min>-999</min>
        <max>0.87</max>
      </li>
      <li>
        <terrain>SoilRich</terrain>
        <min>0.87</min>
        <max>999</max>
      </li>
    </terrainsByFertility>
    <terrainPatchMakers>
      <li>
        <perlinFrequency>0.015</perlinFrequency>
        <thresholds>
          <li>
            <terrain>MossyTerrain</terrain>
            <min>0.0</min>
            <max>0.32</max>
          </li>
          <li>
            <terrain>SoilRich</terrain>
            <min>0.32</min>
            <max>0.80</max>
          </li>
          <li>
            <terrain>Mud</terrain>
            <min>0.80</min>
            <max>0.93</max>
          </li>
          <li>
            <terrain>WaterShallow</terrain>
            <min>0.93</min>
            <max>1.06</max>
          </li>
          <li>
            <terrain>WaterDeep</terrain>
            <min>1.06</min>
            <max>999</max>
          </li>
        </thresholds>
      </li>
    </terrainPatchMakers>
    <baseWeatherCommonalities>
      <Clear>17</Clear>
      <Fog>3</Fog>
      <Rain>2</Rain>
      <DryThunderstorm>1</DryThunderstorm>
      <RainyThunderstorm>1</RainyThunderstorm>
      <FoggyRain>2</FoggyRain>
      <SnowGentle>3</SnowGentle>
      <SnowHard>4</SnowHard>
      <GrayPall MayRequire="Ludeon.RimWorld.Anomaly">1</GrayPall>
      <Overcast MayRequire="Ludeon.RimWorld.Odyssey">2</Overcast>
      <Blizzard MayRequire="Ludeon.RimWorld.Odyssey">2</Blizzard>
      <TorrentialRain MayRequire="Ludeon.RimWorld.Odyssey">0.5</TorrentialRain>
    </baseWeatherCommonalities>
    <wildPlants>
      <Plant_Grass>0.80</Plant_Grass>
      <Plant_TallGrass>0.80</Plant_TallGrass>
      <Plant_Bush>0.4</Plant_Bush>
      <Plant_Brambles>1.0</Plant_Brambles>
      <Plant_Berry>0.30</Plant_Berry>
      <Plant_Fireweed MayRequire="Ludeon.RimWorld.Odyssey">0.1</Plant_Fireweed>
      <Plant_Reeds MayRequire="Ludeon.RimWorld.Odyssey">1</Plant_Reeds>
      <Plant_Bulrush MayRequire="Ludeon.RimWorld.Odyssey">0.2</Plant_Bulrush>
      <Plant_HealrootWild>0.08</Plant_HealrootWild>
      <!-- <DankPyon_Plant_MindwortWild>0.03</DankPyon_Plant_MindwortWild> -->
      <!-- <DankPyon_Plant_PoppyWild>0.03</DankPyon_Plant_PoppyWild> -->
      <!-- <DankPyon_Plant_FleawortWild>0.038</DankPyon_Plant_FleawortWild> -->
      <!-- <DankPyon_Plant_FlyAgaricWild>0.03</DankPyon_Plant_FlyAgaricWild> -->
      <!-- Trees -->
      <MOL_GreatWillow>0.04</MOL_GreatWillow>
      <Plant_TreeWillow>0.04</Plant_TreeWillow>
      <MOL_GreatOak>0.025</MOL_GreatOak>
      <MOL_GreatIter>0.025</MOL_GreatIter>
      <MOL_GreatFir>0.15</MOL_GreatFir>
      <Plant_TreeOak>0.25</Plant_TreeOak>
      <Plant_TreeMaple>0.25</Plant_TreeMaple>
      <Plant_TreePoplar>0.35</Plant_TreePoplar>
    </wildPlants>
    <wildAnimals>
      <Hare>1.00</Hare>
      <Squirrel>1.00</Squirrel>
      <Rat>0.8</Rat>
      <Deer>0.5</Deer>
      <WildBoar>0.5</WildBoar>
      <Turkey>0.5</Turkey>
      <Raccoon>0.4</Raccoon>
      <Tortoise>0.4</Tortoise>
      <Ibex>0.3</Ibex>
      <Boomrat>0.25</Boomrat>
      <Muffalo>0.2</Muffalo>
      <Boomalope>0.2</Boomalope>
      <Elk>0.3</Elk>
      <Wolf_Timber>0.04</Wolf_Timber>
      <Fox_Red>0.04</Fox_Red>
      <Megasloth>0.04</Megasloth>
      <Bear_Grizzly>0.022</Bear_Grizzly>
      <Warg>0.012</Warg>
      <Cougar>0.012</Cougar>
      <Lynx>0.01</Lynx>
      <Moose MayRequire="Ludeon.RimWorld.Odyssey">0.2</Moose>
      <Wolf_Great MayRequire="Ludeon.RimWorld.Odyssey">0.02</Wolf_Great>
      <Porcupine MayRequire="Ludeon.RimWorld.Odyssey">0.2</Porcupine>
      <Badger MayRequire="Ludeon.RimWorld.Odyssey">0.2</Badger>
      <Wolverine MayRequire="Ludeon.RimWorld.Odyssey">0.07</Wolverine>
      <Mink MayRequire="Ludeon.RimWorld.Odyssey">0.2</Mink>
      <Heron MayRequire="Ludeon.RimWorld.Odyssey">0.2</Heron>
      <Swan MayRequire="Ludeon.RimWorld.Odyssey">0.1</Swan>
      <Sparrow MayRequire="Ludeon.RimWorld.Odyssey">0.5</Sparrow>
      <Crow MayRequire="Ludeon.RimWorld.Odyssey">0.5</Crow>
      <MOL_Rox>0.02</MOL_Rox>
      <MOL_LargeCobra_Cave>0.07</MOL_LargeCobra_Cave>
      <MOL_LargeCobra_Forest>0.05</MOL_LargeCobra_Forest>
      <MOL_GiantConstrictor_Cave>0.06</MOL_GiantConstrictor_Cave>
      <MOL_GiantConstrictor_Forest>0.07</MOL_GiantConstrictor_Forest>
      <MOL_DireBoar>0.5</MOL_DireBoar>
      <MOL_NorthernBoar>0.3</MOL_NorthernBoar>
      <MOL_Direwolf>0.08</MOL_Direwolf>
      <MOL_Sapling_Dark>0.02</MOL_Sapling_Dark>
    </wildAnimals>
    <pollutionWildAnimals MayRequire="Ludeon.RimWorld.Biotech">
      <WasteRat MayRequire="Ludeon.RimWorld.Biotech">1</WasteRat>
      <Rat>0.5</Rat>
      <Raccoon>0.25</Raccoon>
      <Warg>0.05</Warg>
    </pollutionWildAnimals>
    <coastalWildAnimals>
      <Walrus MayRequire="Ludeon.RimWorld.Odyssey">0.1</Walrus>
      <SeaLion MayRequire="Ludeon.RimWorld.Odyssey">0.1</SeaLion>
      <Otter MayRequire="Ludeon.RimWorld.Odyssey">0.1</Otter>
    </coastalWildAnimals>
    <allowedPackAnimals>
      <li>MOL_Rox</li>
      <li>Muffalo</li>
      <li>Alpaca</li>
    </allowedPackAnimals>
  </BiomeDef>
</Defs>
