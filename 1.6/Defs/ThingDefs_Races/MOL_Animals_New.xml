  <!-- ========================== Giant Cave Constrictor ================================= -->
  <ThingDef ParentName="AnimalThingBase">
    <defName>MOL_GiantConstrictor_Cave</defName>
    <label>giant cave constrictor</label>
    <description>Massive constrictor snake adapted for subterranean hunting. Enhanced musculature allows it to overpower prey far larger than baseline constrictors. Prefers humid cave systems and deep forest undergrowth.</description>
    <statBases>
      <MoveSpeed>5.2</MoveSpeed>
      <MarketValue>3000</MarketValue>
      <ComfyTemperatureMin>0</ComfyTemperatureMin>
      <ComfyTemperatureMax>60</ComfyTemperatureMax>
      <ArmorRating_Sharp>0.67</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.44</ArmorRating_Blunt>
      <ArmorRating_Heat>0.62</ArmorRating_Heat>
      <Wildness>0.95</Wildness>
    </statBases>
    <uiIconScale>1.6</uiIconScale>
    <tools>
      <li>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>24</power>
        <cooldownTime>2.5</cooldownTime>
        <linkedBodyPartsGroup>Mouth</linkedBodyPartsGroup>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>15</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <race>
      <body>Snake</body>
      <leatherDef>Leather_Rhinoceros</leatherDef>
      <predator>true</predator>
      <maxPreyBodySize>3</maxPreyBodySize>
      <baseBodySize>3.65</baseBodySize>
      <baseHungerRate>0.5</baseHungerRate>
      <baseHealthScale>6.7</baseHealthScale>
      <foodType>CarnivoreAnimal, OvivoreAnimal</foodType>
      <meatLabel>constrictor meat</meatLabel>
      <petness>0.05</petness>
      <manhunterOnTameFailChance>0.50</manhunterOnTameFailChance>
      <manhunterOnDamageChance>1.00</manhunterOnDamageChance>
      <nameOnTameChance>1</nameOnTameChance>
      <lifeExpectancy>200</lifeExpectancy>
      <useMeatFrom>MOL_GiantConstrictor_Forest</useMeatFrom>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.75</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>1.332</minAge>
          <soundWounded>Pawn_Cobra_Wounded</soundWounded>
          <soundDeath>Pawn_Cobra_Death</soundDeath>
          <soundCall>Pawn_Cobra_Call</soundCall>
          <soundAngry>Pawn_Cobra_Angry</soundAngry>
        </li>
