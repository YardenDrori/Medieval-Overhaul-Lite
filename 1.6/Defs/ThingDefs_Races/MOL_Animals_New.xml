<Defs>
  <!-- ========================== Large Cave Cobra ================================= -->
  <ThingDef ParentName="AnimalThingBase">
    <defName>MOL_LargeCobra_Cave</defName>
    <label>large cave cobra</label>
    <description>Cobra variant with enhanced low-light adaptation genes. Larger and more aggressive than baseline specimens, likely from early bioweapons research. Prefers cave systems and shaded areas. Venom remains highly dangerous.</description>
    <statBases>
      <MoveSpeed>4.5</MoveSpeed>
      <MarketValue>350</MarketValue>
      <ComfyTemperatureMin>0</ComfyTemperatureMin>
      <ComfyTemperatureMax>60</ComfyTemperatureMax>
      <Wildness>0.75</Wildness>
    </statBases>
    <uiIconScale>1.5</uiIconScale>
    <tools>
      <li>
        <label>venom-fangs</label>
        <capacities>
          <li>ToxicBite</li>
        </capacities>
        <power>14</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>Mouth</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>3</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <race>
      <body>Snake</body>
      <predator>true</predator>
      <maxPreyBodySize>0.80</maxPreyBodySize>
      <baseBodySize>0.55</baseBodySize>
      <baseHungerRate>0.1</baseHungerRate>
      <baseHealthScale>0.75</baseHealthScale>
      <foodType>CarnivoreAnimal, OvivoreAnimal</foodType>
      <leatherDef>Leather_Lizard</leatherDef>
      <useMeatFrom>Cobra</useMeatFrom>
      <petness>0.05</petness>
      <manhunterOnTameFailChance>0.10</manhunterOnTameFailChance>
      <manhunterOnDamageChance>0.50</manhunterOnDamageChance>
      <nameOnTameChance>1</nameOnTameChance>
      <lifeExpectancy>30</lifeExpectancy>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.15</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.5</minAge>
          <soundWounded>Pawn_Cobra_Wounded</soundWounded>
          <soundDeath>Pawn_Cobra_Death</soundDeath>
          <soundCall>Pawn_Cobra_Call</soundCall>
          <soundAngry>Pawn_Cobra_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
    </race>
    <tradeTags>
      <li>AnimalUncommon</li>
      <li>AnimalFighter</li>
    </tradeTags>
  </ThingDef>
