<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <BodyDef>
    <defName>MOL_SchratBody</defName>
    <label>schrat</label>
    <corePart>
      <def>MechanicalThorax</def>
      <customLabel>trunk</customLabel>
      <height>Middle</height>
      <depth>Outside</depth>
      <parts>
        <li>
          <def>ArtificialBrain</def>
          <customLabel>heart of the forest</customLabel>
          <coverage>0.06</coverage>
          <depth>Inside</depth>
        </li>
        <li>
          <def>MechanicalNeck</def>
          <customLabel>upper trunk</customLabel>
          <coverage>0.10</coverage>
          <height>Top</height>
          <parts>
            <li>
              <def>MechanicalHead</def>
              <customLabel>gnarled head</customLabel>
              <coverage>0.80</coverage>
              <groups>
                <li>HeadAttackTool</li>
              </groups>
              <parts>
                <li>
                  <def>SightSensor</def>
                  <customLabel>left eye knot</customLabel>
                  <coverage>0.13</coverage>
                </li>
                <li>
                  <def>SightSensor</def>
                  <customLabel>right eye knot</customLabel>
                  <coverage>0.13</coverage>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalShoulder</def>
          <customLabel>left branch joint</customLabel>
          <coverage>0.17</coverage>
          <parts>
            <li>
              <def>MechanicalArm</def>
              <customLabel>left branch arm</customLabel>
              <coverage>0.85</coverage>
              <parts>
                <li>
                  <def>MechanicalHand</def>
                  <customLabel>left branch hand</customLabel>
                  <coverage>0.2</coverage>
                  <height>Bottom</height>
                  <groups>
                    <li>LeftHand</li>
                  </groups>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalShoulder</def>
          <customLabel>right branch joint</customLabel>
          <coverage>0.17</coverage>
          <parts>
            <li>
              <def>MechanicalArm</def>
              <customLabel>right branch arm</customLabel>
              <coverage>0.85</coverage>
              <parts>
                <li>
                  <def>MechanicalHand</def>
                  <customLabel>right branch hand</customLabel>
                  <coverage>0.2</coverage>
                  <height>Bottom</height>
                  <groups>
                    <li>RightHand</li>
                  </groups>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalLeg</def>
          <customLabel>left root leg</customLabel>
          <coverage>0.2</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>MechanicalFoot</def>
              <customLabel>left root foot</customLabel>
              <coverage>0.2</coverage>
            </li>
          </parts>
        </li>
        <li>
          <def>MechanicalLeg</def>
          <customLabel>right root leg</customLabel>
          <coverage>0.2</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>MechanicalFoot</def>
              <customLabel>right root foot</customLabel>
              <coverage>0.2</coverage>
            </li>
          </parts>
        </li>
      </parts>
    </corePart>
  </BodyDef>

</Defs>
